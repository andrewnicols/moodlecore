{
    "name": "andrewnicols/moodlecore",
    "license": "GPL-3.0-or-later",
    "description": "Moodle - the world's open source learning platform",
    "type": "project",
    "homepage": "https://moodle.org",
    "require-dev": {
        "phpunit/phpunit": "^9.6.18",
        "mikey179/vfsstream": "1.6.*",
        "behat/mink": "^1.11.0",
        "friends-of-behat/mink-extension": "^2.7.5",
        "behat/mink-browserkit-driver": "^2.2.0",
        "symfony/process": "^4.4 || ^5.0 || ^6.0 || ^7.0",
        "symfony/http-client": "^4.4 || ^5.0 || ^6.0 || ^7.0",
        "symfony/mime": "^4.4 || ^5.0 || ^6.0 || ^7.0",
        "behat/behat": "3.14.*",
        "oleg-andreyev/mink-phpwebdriver": "1.3.*",
        "filp/whoops": "^2.15"
    },
    "autoload-dev": {
        "psr-0": {
            "Moodle\\BehatExtension": "lib/behat/extension/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1.0",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "ext-curl": "*",
        "ext-openssl": "*",
        "ext-ctype": "*",
        "ext-zip": "*",
        "ext-zlib": "*",
        "ext-gd": "*",
        "ext-simplexml": "*",
        "ext-spl": "*",
        "ext-pcre": "*",
        "ext-dom": "*",
        "ext-xml": "*",
        "ext-xmlreader": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-hash": "*",
        "ext-fileinfo": "*",
        "ext-sodium": "*",
        "slim/slim": "^4.14",
        "spatie/php-cloneable": "^1.0",
        "php-di/php-di": "^7.0",
        "psr/clock": "^1.0",
        "psr/event-dispatcher": "^1.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.1",
        "psr/http-message": "^2.0",
        "psr/log": "^3.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/simple-cache": "^3.0",
        "guzzlehttp/guzzle": "^7.8",
        "kevinrob/guzzle-cache-middleware": "^5.1",
        "openspout/openspout": "^4.25",
        "firebase/php-jwt": "^6.10",
        "aws/aws-sdk-php": "^3.318",
        "myclabs/php-enum": "^1.8",
        "phpmailer/phpmailer": "^6.9",
        "html2text/html2text": "^4.3",
        "sabberworm/php-css-parser": "^8.6",
        "geoip2/geoip2": "^3.0",
        "maxmind-db/reader": "^1.11",
        "matthiasmullie/minify": "^1.3",
        "giggsey/libphonenumber-for-php-lite": "^8.13",
        "redeyeventures/geopattern": "^1.1",
        "scssphp/scssphp": "^1.13",
        "maennchen/zipstream-php": "^3.1",
        "ezyang/htmlpurifier": "^4.17",
        "lbuchs/webauthn": "^2.1",
        "mtdowling/jmespath.php": "^2.7",
        "phpoffice/phpspreadsheet": "^1.29",
        "phpxmlrpc/phpxmlrpc": "^4.10"
    },
    "suggest": {
        "ext-mysqli": "Needed when Moodle uses MySQL or MariaDB database.",
        "ext-pgsql": "Needed when Moodle uses PostgreSQL database.",
        "ext-sqlsrv": "Needed when Moodle uses MS SQL Server database.",
        "ext-oci8": "Needed when Moodle uses Oracle database.",
        "ext-tokenizer": "Enabling Tokenizer PHP extension is recommended, it improves Moodle Networking functionality.",
        "ext-soap": "Enabling SOAP PHP extension is useful for web services and some plugins.",
        "ext-exif": "Enabling Exif PHP extension is recommended, it is used by Moodle to parse image meta data."
    }
}
